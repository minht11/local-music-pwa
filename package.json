{
	"name": "local-music-pwa-next",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"sideEffects": false,
	"scripts": {
		"prepare": "svelte-kit sync",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"biome-check": "biome check ./src",
		"biome-fix": "biome check ./src --write",
		"prettier-check": "prettier --check ./src",
		"prettier-fix": "prettier --write ./src",
		"compile-i18n": "paraglide-js compile --project ./project.inlang",
		"test": "vitest run",
		"coverage": "vitest run --coverage"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.0",
		"@inlang/paraglide-js": "2.2.0",
		"@sveltejs/adapter-static": "^3.0.9",
		"@sveltejs/kit": "^2.36.1",
		"@sveltejs/vite-plugin-svelte": "6.1.3",
		"@tailwindcss/vite": "^4.1.12",
		"@types/node": "^24.3.0",
		"@vitest/coverage-v8": "^3.2.4",
		"fake-indexeddb": "^6.1.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"svelte": "5.38.2",
		"svelte-check": "^4.3.1",
		"tailwindcss": "^4.1.12",
		"terser": "^5.43.1",
		"typescript": "^5.9.2",
		"unplugin-auto-import": "^20.0.0",
		"vite": "npm:rolldown-vite@7.1.4",
		"vitest": "^3.2.4"
	},
	"dependencies": {
		"@floating-ui/dom": "^1.7.4",
		"@material/material-color-utilities": "^0.3.0",
		"@resvg/resvg-js": "^2.6.2",
		"@sveltejs/enhanced-img": "^0.8.1",
		"@tanstack/svelte-virtual": "^3.13.12",
		"@types/dom-navigation": "^1.0.6",
		"@types/wicg-file-system-access": "^2023.10.6",
		"idb": "^8.0.3",
		"music-metadata": "^11.8.3",
		"socket.io-client": "^4.8.1",
		"tiny-invariant": "^1.3.3",
		"valibot": "1.1.0",
		"weak-lru-cache": "^1.2.2"
	},
	"engines": {
		"node": ">=22.19.0"
	},
	"packageManager": "pnpm@10.15.0",
	"pnpm": {
		"overrides": {
			"vite": "npm:rolldown-vite@7.1.4",
			"@ianvs/prettier-plugin-sort-imports": "-",
			"@trivago/prettier-plugin-sort-imports": "-",
			"prettier-plugin-organize-imports": "-",
			"prettier-plugin-css-order": "-",
			"prettier-plugin-import-sort": "-",
			"prettier-plugin-jsdoc": "-",
			"prettier-plugin-multiline-arrays": "-",
			"prettier-plugin-organize-attributes": "-",
			"prettier-plugin-style-order": "-",
			"prettier-plugin-sort-imports": "-"
		},
		"patchedDependencies": {
			"@material/material-color-utilities": "patches/@material__material-color-utilities.patch",
			"debug": "patches/debug.patch"
		},
		"onlyBuiltDependencies": [
			"@biomejs/biome",
			"@tailwindcss/oxide",
			"esbuild",
			"sharp"
		]
	}
}
