{
	"name": "local-music-pwa-next",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"sideEffects": false,
	"scripts": {
		"prepare": "svelte-kit sync",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"biome-check": "biome check ./src",
		"biome-fix": "biome check ./src --write",
		"prettier-check": "prettier --check ./src",
		"prettier-fix": "prettier --write ./src",
		"compile-i18n": "paraglide-js compile --project ./project.inlang",
		"test": "vitest run",
		"coverage": "vitest run --coverage"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.8",
		"@inlang/paraglide-js": "2.6.0",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.49.1",
		"@sveltejs/vite-plugin-svelte": "6.2.1",
		"@tailwindcss/vite": "^4.1.17",
		"@types/node": "^24.10.1",
		"@vitest/coverage-v8": "^4.0.15",
		"fake-indexeddb": "^6.2.5",
		"prettier": "^3.7.4",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"svelte": "5.45.6",
		"svelte-check": "^4.3.4",
		"tailwindcss": "^4.1.17",
		"typescript": "^5.9.3",
		"unplugin-auto-import": "^20.3.0",
		"vite": "8.0.0-beta.0",
		"vite-imagetools": "^9.0.2",
		"vitest": "^4.0.15"
	},
	"dependencies": {
		"@floating-ui/dom": "^1.7.4",
		"@material/material-color-utilities": "^0.3.0",
		"@resvg/resvg-js": "^2.6.2",
		"@tanstack/svelte-virtual": "^3.13.12",
		"@types/dom-navigation": "^1.0.6",
		"@types/wicg-file-system-access": "^2023.10.7",
		"idb": "^8.0.3",
		"music-metadata": "^11.10.3",
		"tiny-invariant": "^1.3.3",
		"valibot": "1.2.0",
		"weak-lru-cache": "^1.2.2"
	},
	"engines": {
		"node": "24.5.0"
	},
	"packageManager": "pnpm@10.19.0",
	"pnpm": {
		"overrides": {
			"vite": "npm:vite@8.0.0-beta.0",
			"@ianvs/prettier-plugin-sort-imports": "-",
			"@trivago/prettier-plugin-sort-imports": "-",
			"prettier-plugin-organize-imports": "-",
			"prettier-plugin-css-order": "-",
			"prettier-plugin-import-sort": "-",
			"prettier-plugin-jsdoc": "-",
			"prettier-plugin-multiline-arrays": "-",
			"prettier-plugin-organize-attributes": "-",
			"prettier-plugin-style-order": "-",
			"prettier-plugin-sort-imports": "-"
		},
		"patchedDependencies": {
			"@material/material-color-utilities": "patches/@material__material-color-utilities.patch",
			"debug": "patches/debug.patch"
		},
		"onlyBuiltDependencies": [
			"@biomejs/biome",
			"@tailwindcss/oxide",
			"esbuild",
			"sharp"
		]
	}
}
